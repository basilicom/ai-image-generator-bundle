{
  "name": "basilicom/ai-image-generator-bundle",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "Basilicom\\AiImageGeneratorBundle\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-imagick": "*",
    "pimcore/pimcore": "^10.0 || ^11.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.13",
    "phpunit/phpunit": "^10.4"
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ],
    "psr-4": {
      "Tests\\": "tests/",
      "Tests\\Unit\\": "tests/Unit/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Basilicom\\AiImageGeneratorBundle"
      ]
    }
  }
}
